# Supabase Environment Variables Example
# Copy this file to .env.local and fill in your values

# Supabase API Configuration
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres
POSTGRES_PASSWORD=postgres

# Optional: OpenAI API Key for Supabase AI features
# OPENAI_API_KEY=your_openai_api_key_here

# BTPC Specific Configuration
BTPC_NETWORK=testnet
BTPC_NODE_URL=http://127.0.0.1:8332
BTPC_NODE_RPC_USER=btpc
BTPC_NODE_RPC_PASSWORD=change_this_password

# Email Configuration
SMTP_HOST=127.0.0.1
SMTP_PORT=54325